#include <main.h>


//Khai bao nut

#define BUT_1 input(PIN_B4)
#define BUT_2 input(PIN_B5)

//Khai bao den

#define LED_1 PIN_D0
#define LED_2 PIN_D1

int a=0,b=0;

void main()
{
   output_d(0xFF);
   if(BUT_1==0)
   {
      while(BUT_1==0)
      {delay_ms(1);}
      if(a==0)
      {
         output_d(0x00);
         a=1;
      }
      if(a==1)
      {
         output_bit(LED_1,1);
         a=0;
      }
   }
}
